@prefix pathocert: <https://w3id.org/def/pathocert/> .
@base <https://w3id.org/def/pathocert/> .

<https://w3id.org/def/pathocert/> rdf:type owl:Ontology ;
                                      dcterms:title "Incidence, Emergency Response and Risk Assessment model"@en ;
                                      dcterms:description "A SAREF4WATR extenstion to model incidence, emergency response and Risk Assessment modelling. The main intention of the ontology is to interlink water management information with water quality and risk assessment. This ontology has been elaborated under the PATHOCERT H2020 project."@en ;
                                      dcterms:publisher <https://pathocert.eu/> ;
                                      dcterms:creator "Aitor Corchero";
                                      owl:versionIRI <https://w3id.org/def/pathocert/v0.0.1/> ;
                                      owl:versionInfo "v0.0.1" ;
                                      dcterms:modified "2021-07-05"^^xsd:date ;
                                      dcterms:issued "2021-07-05"^^xsd:date ;
                                      dcterms:license <https://opensource.org/licenses/ISC> ;
                                      dcterms:source <https://github.com/aolite/PATHOCERTOntology> ;
                                      vann:preferredNamespacePrefix "pathocert" ;
                                      vann:preferredNamespaceUri "https://w3id.org/def/pathocert/" .


